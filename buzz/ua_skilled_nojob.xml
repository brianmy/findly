<?xml version="1.0" encoding="UTF-8" ?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:mergeformat="findly.Merge.JobmailMergeProvider">
<xsl:param name="ImageRootPath" />
<xsl:param name="ClickRootPath" />
<xsl:param name="EmailId" />
<xsl:param name="WeatherEngineRootPath" />
<xsl:template match="Merge">
<xsl:text disable-output-escaping='yes'>&lt;!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"></xsl:text>

<html>
<head>
<meta name="viewport" content="width=device-width"/>
<style type="text/css">
body, .body {
	background: #333333;
}

.ExternalClass { 
  width:100%;
} 

.ExternalClass, 
.ExternalClass p, 
.ExternalClass span, 
.ExternalClass font, 
.ExternalClass td, 
.ExternalClass div { 
  line-height: 100%; 
} 

table {
  mso-table-lspace:0pt;
  mso-table-rspace:0pt;
}

.apple-links a {
	color:#999999;
	text-decoration: none;
}

.apple-links-white a {
	color:#ffffff;
	text-decoration: none;
}


@media only screen and (max-width: 580px){
.flex-size { 
	width: 100% !important;
	max-width: 100% !important;
}

.apply-btn {
	padding: 10px 0 0 !important;
}

.hide { display: none;	}

.vid-block {
	display: block;
	padding: 20px 0 !important;
}

.social-block { 
	display: block;
	padding: 20px 0 !important;
}

}
</style>

<xsl:comment><![CDATA[[if gte mso 9]>
    <style type="text/css">
		.table-border-fix {
			width: 192px !important;
		}
		
		.ol-social-size {
			width: 260px !important;
		}
		
		.ol-vid-size {
			width: 240px !important;
		}
    </style>
<![endif]]]></xsl:comment>

</head>
<body bgcolor="#333333" marginheight="0" marginwidth="0" style="-webkit-text-size-adjust: 100%; -ms-text-size-adjust: 100%; margin: 0; padding: 0;">
